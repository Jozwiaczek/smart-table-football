{
  "name": "stf-ui-components",
  "version": "0.1.0",
  "private": true,
  "description": "Smart Table Football sharable UI Components",
  "keywords": [
    "Components",
    "UI",
    "Smart Table Football",
    "STF",
    "Storybook",
    "table football",
    "table soccer",
    "foosball"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Jozwiaczek/smart-table-football",
    "directory": "packages/ui-components"
  },
  "author": {
    "name": "Jakub Jóźwiak",
    "email": "kubencki@gmail.com"
  },
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rimraf dist && babel src -d dist --config-file ./babelConfig/.babelrc",
    "build:storybook": "rimraf storybook-static && build-storybook -s public --no-dll",
    "build:watch": "yarn build --watch",
    "postinstall": "yarn build",
    "lint": "scripty",
    "lint:fix": "yarn lint --fix",
    "storybook": "start-storybook -p 6006 -s public --no-dll"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "8.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.2.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-controls": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/node-logger": "^6.0.28",
    "@storybook/react": "^6.0.28"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x",
    "styled-components": "5.2.0"
  },
  "scripty": {
    "path": "../../scripts"
  }
}
